{
    "name": "artyomnar/yii2-core-api-client",

    "type": "yii2-extension",

    "description": "wrapper on yii2-httplcient for work with WP Core API",
    "keywords": ["yii2", "api", "wrapper", "helper"],
    "license": "MIT",
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/artyomnar94/yii2-core-api-client/issues",
        "wiki": "https://github.com/artyomnar94/yii2-core-api-client/wiki",
        "source": "https://github.com/artyomnar94/yii2-core-api-client"
    },
    "authors": [
        {
            "name": "Artyom Narmagambetov",
            "email": "artyomnar94@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CoreApiClient\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-httpclient": "^2.0"
    }
}
